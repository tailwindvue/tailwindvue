{
    "name": "@tailwindvue/tailwindvue",
    "version": "0.1.5",
    "description": "A collection of VueJs components made with TailwindCSS",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://www.tailwindvue.com",
    "keywords": [
        "Vue",
        "VueJs",
        "Tailwind",
        "TailwindCSS",
        "Components"
    ],
    "author": "Carl Cassar <hello@carlcassar.com>",
    "main": "dist/tailwindvue.umd.min.js",
    "module": "dist/tailwindvue.umd.js",
    "unpkg": "dist/tailwindvue.common.js",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/tailwindvue/tailwindvue"
    },
    "files": [
        "src",
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "test": "vue-cli-service test:unit",
        "build-lib": "vue-cli-service build --target lib --name tailwindvue src/main.js",
        "watch": "vue-cli-service build --watch --target lib --name tailwindvue src/main.js"
    },
    "dependencies": {
        "core-js": "^3.6.4",
        "deepmerge": "^4.2.2",
        "tailwindcss": "^1.2.0",
        "vue": "^2.6.11"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.2.0",
        "@vue/cli-plugin-unit-jest": "~4.2.0",
        "@vue/cli-service": "~4.2.0",
        "@vue/test-utils": "1.0.0-beta.31",
        "vue-template-compiler": "^2.6.11"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "jest": {
        "preset": "@vue/cli-plugin-unit-jest",
        "moduleNameMapper": {
            "Components/(.*)$": "<rootDir>/src/components/$1"
        }
    }
}
